<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Epsi.vn</title>
   
   

    <link href="http://fonts.googleapis.com/css?family=Open+Sans:400italic,700italic,400,700&#038;subset=latin,latin-ext" rel="stylesheet" type="text/css" media="all" />
    <link href='https://fonts.googleapis.com/css?family=Roboto+Mono:400,100,300,500,700,100italic,300italic,400italic,500italic,700italic' rel='stylesheet' type='text/css'>
    <link href="http://fonts.googleapis.com/css?family=Roboto:400,700,900" rel="stylesheet" type="text/css" media="all" />
    @Styles.Render("~/bundles/css/botstrap")
    @Styles.Render("~/bundles/css/lib")
    @Styles.Render("~/bundles/css/epsi")
    
   <link href="/Content/font-awesome.min.css" rel="stylesheet" type="text/css" />
    <link href="/Scripts/lib/slick/slick.css" rel="stylesheet" type="text/css" />
    <link href="/Scripts/lib/slick/slick-theme.css" rel="stylesheet" type="text/css" />

    <!--[if lt IE 9]>
    <script src="/Scripts/lib/html5shiv.min.js"></script>
    <script src="/Scripts/lib/respond.min.js"></script>
    <![endif]-->
    @Scripts.Render("~/bundles/modernizr")


</head>
<body>
    <div class="wrapper ">
        <div class="page-wrapper">
            @{Html.RenderPartial("_header");}
            <!-- Main Container -->
            <div class="main-container">
                @RenderBody()                
            </div>
            <!-- End Main Container -->
            @{Html.RenderPartial("_footer");}
        </div>
    </div>
    
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
  

    <script src="/Scripts/lib/slick/slick.min.js"></script>

    <script type="text/javascript" src="/Scripts/lib/rs-plugin/js/jquery.themepunch.tools.min.js"></script>
    <script type="text/javascript" src="/Scripts/lib/rs-plugin/js/jquery.themepunch.revolution.min.js"></script>
    <script type="text/javascript" src="/Scripts/lib/rs-plugin/rs.home.js"></script>
    <script type="text/javascript" src="/Scripts/lib/chosen/chosen.jquery.js"></script>
    <script type="text/javascript" src="/Scripts/lib/chosen/chosen.proto.min.js"></script>
    <script src="/Scripts/lib/prettyPhoto/js/jquery.prettyPhoto.js"></script>

    <script type="text/javascript" src="/Scripts/lib/ntm/jquery.ntm.js"></script>
    <script type="text/javascript" src="/Scripts/core.js"></script>
    <!-- Google Map js -->
    <script type='text/javascript' src="http://maps.google.com/maps/api/js?sensor=false"></script>
    <script type="text/javascript" src="/Scripts/jquery.gmap.js"></script>



    <script type="text/javascript">
        $('#map').gMap({
            scrollwheel: false,
            zoom: 17,
            address: "Pall Mall, London England",
            markers: [{
                address: "Pall Mall, London England",
                html: '',
                icon: {
                    image: "images/fashion/style/marker.png",
                    iconsize: [32, 47],
                    iconanchor: [32, 47]
                },
                popup: true
            }]
        });
    </script>
    @RenderSection("scripts", required: false)
</body>
</html>
