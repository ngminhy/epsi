@model IEnumerable<epsi.Models.Category>
<ul class="sub-menu">
@foreach (var item in Model)
{
    <li class="children-menu">
        <a href="/san-pham/@Url.Content(item.PageURL)">@item.Name</a>
        @{ Html.RenderAction("MenuMobileCategoryChild", "Home", new { parentId = item.CategoryId });}

    </li>
}


</ul>
